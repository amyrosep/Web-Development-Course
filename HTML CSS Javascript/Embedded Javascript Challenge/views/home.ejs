<%- include("partials/header"); %>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% for(let i = 0; i < posts.length; i++) { %>
  <h2><%= posts[i].title %></h2>
  <p>
    <span class="truncated"><%= posts[i].body %></span>
    <a class="readmore" href="posts/<%= posts[i]._id %>">Read More</a>
  </p>
<% } %>

<%- include("partials/footer"); %>
