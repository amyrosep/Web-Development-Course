
<%- include("header") -%>

<!-- < % = variableName % > Is an ejs marker for replacement -->
<div class="box" id="heading">
  <h1><%=listTitle%></h1>
</div>

<div class="box">
  <% for (let i=0; i<listItems.length; i++) { %>
  <div class="item">
    <input type="checkbox" name="" value="">

    <p><%-listItems[i]%></p>
  </div>
    <% } %>

  <form class="" action="/" method="post">
    <input type="text" name="todoItem" placeholder="New Task" autocomplete="off">
    <button type="submit" name="list" value=<%= listTitle %>>+</button>
  </form>

</div>

<%- include("footer") -%>
